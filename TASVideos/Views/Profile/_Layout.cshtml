@{
	Layout = "/Views/Shared/_Layout.cshtml";
}
<h2>Your account profile</h2>
<div>
	<h4>Change your account settings</h4>
	<hr />
	<row>
		<div class="col-lg-3">
			@await Html.PartialAsync("_ProfileNav")
		</div>
		<div class="col-lg-9">
			@RenderBody()
		</div>
	</row>
</div>
@section Scripts {
	@RenderSection("Scripts", required: false)
}
