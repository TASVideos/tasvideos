{
  "BaseUrl": "https://tasvideos.org",
  "EnableGzipCompression": "False",
  "MinimumHoursBeforeJudgment": 72,
  "SubmissionRate": {
    "Submissions": 3,
    "Days": 7
  },
  "StartupStrategy": "Minimal",
  "CacheSettings": {
    "CacheType": "Redis",
    "CacheDurationInSeconds": "3600",
    "ConnectionString": "localhost"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft.AspNetCore.Identity": "Error",
        "Microsoft.AspNetCore.Mvc.ViewFeatures.Filters.AutoValidateAntiforgeryTokenAuthorizationFilter": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "/home/tasvideos/logs/applogs.json",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "webOptimizer": {
    "enableCaching": true,
    "enableTagHelperBundling": true
  }
}
