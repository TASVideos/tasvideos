@model IEnumerable<WikiUserEntry>

@foreach (var user in Model
	.OrderByDescending(u => u.PublicationCount)
	.ThenBy(u => u.SubmissionCount))
{
	<div class="card-userinfo">
		<div class="card-header-userinfo">
			<profile-link username="@user.UserName"></profile-link>
		</div>
		<div class="card-body-userinfo">
			Publications: @user.PublicationCount
			<br />
			Submissions: @user.SubmissionCount
		</div>
	</div>
}
