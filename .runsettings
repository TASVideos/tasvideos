    </DataCollectors>
  </DataCollectionRunSettings>
  <TestRunParameters>
    <Parameter name="PostgresTestsConnection" value="Host=127.0.0.1;Port=5432;Database=TASVideosTests;User Id=postgres;Password=postgres;Include Error Detail=true" />
  </TestRunParameters>
</RunSettings>
