{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "npm install",
            "command": "npm",
            "type": "shell",
            "args": [
                "install"
            ],
            "options": {
                "cwd": "${workspaceFolder}/TASVideos"
            }
        },
        {
            "label": "npm run build",
            "command": "npm",
            "type": "shell",
            "args": [
                "run",
                "build"
            ],
            "options": {
                "cwd": "${workspaceFolder}/TASVideos"
            },
            "dependsOn": "npm install"
        },
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/TASVideos/TASVideos.csproj"
            ],
            "problemMatcher": "$msCompile",
            "dependsOn": "npm run build"
		},
		{
            "label": "build_forumengine",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/TASVideos.ForumEngineTempTest/TASVideos.ForumEngineTempTest.csproj"
            ],
            "problemMatcher": "$msCompile"			
		}
    ]
}